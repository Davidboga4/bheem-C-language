#include <stdio.h>

void main() {
    int a[5][5]={0},i,j,n,c,t=0;
    printf("Enter array size: ");
    scanf("%d", &n);
    printf("Enter A array values:\n");
    for(i=0; i<n; i++){
        for(j=0,c=0; j<n; j++){
            printf("Array Values %d %d:", i,j);
            scanf("%d", &a[i][j]);
            if(a[i][j] == 0)
            c++;
        }
        if(t > c) break;
    }
    i == n ? printf("Upper"):printf("Lower");
}
